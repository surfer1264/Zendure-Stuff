     akkuMin\n    );\n}\n\n// ---------- Status ----------\nnode.status({\n    fill: \"green\",\n    shape: \"dot\",\n    text: `FC:${fc}Wh | SoCmin:${akkuMin}% | Netto:${nettoWh}Wh | Neu:${newSocMin}%`\n});\n\n// ---------- Output ----------\nif (pvForecastEnabled) {\n    return {\n        payload: {\n            value: newSocMin,\n            sensor: socSensor\n        }\n    };\n}\n\nreturn null;\n\n\n",
